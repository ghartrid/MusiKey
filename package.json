    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.0.0",
    "electron": "^35.0.0",
    "esbuild": "^0.27.0",
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "argon2": "^0.44.0"
  },
  "overrides": {
    "tar": "^7.5.8",
    "tmp": "^0.2.4"
  }
